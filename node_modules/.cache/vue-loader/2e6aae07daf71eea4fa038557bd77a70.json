port axios from 'axios'\n    import VueCookies from 'vue-cookies'\n    import Vue from 'vue'\n\n    Vue.use(VueCookies);\n\n    export default {\n        name: \"Home\",\n        data() {\n            return {\n                name: '',\n                list: false,\n                city: Vue.$cookies.get('City'),\n                id: Vue.$cookies.get('Id')\n            }\n        },\n        methods: {\n            search() {\n                /* eslint-disable no-console */\n                console.log(this.name)\n\n                axios\n                    .get(`http://api.openweathermap.org/data/2.5/weather?q=${this.name}&APPID=2e6c186fe7c73137b1b5d687225645a8\n`)\n                    .then((response) => {\n                            if (response.status === 200) {\n                                /* eslint-disable no-console */\n                                console.log(response.data)\n                                Vue.$cookies.set('City',response.data.city);\n                                Vue.$cookies.set('Id', response.data.id);\n                                this.city = Vue.$cookies.get('City')\n                                this.id = Vue.$cookies.get('Id')\n                                this.$store.commit('set', {\n                                    type: \"all\",\n                                    items: response.data,\n\n                                })\n                                ;\n                            }\n                        }\n                        /* eslint-disable no-console */\n                    ).catch(error => console.log(error));\n                this.list = true;\n            }\n        },\n\n    }\n</script>\n\n<style scoped>\n\n\n</style>"]}]}